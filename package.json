{
  "name": "saas-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --ext .js,.ts,.vue ./src",
    "inspect": "vue inspect --mode development --verbose > inspect.js",
    "push:vsa": "git subtree push --prefix=vue-service-app git@gitlab.styd.cn:fe/vue-service-app.git master",
    "push:rx":"git subtree push --prefix=rx-state git@gitlab.styd.cn:fe/rx-state.git master"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.2",
    "ant-design-vue": "1.3.4",
    "fs-extra": "^7.0.1",
    "globby": "^9.0.0",
    "js-cookie": "^2.2.0",
    "lodash-es": "^4.17.11",
    "nprogress": "^0.2.0",
    "rxjs": "^6.4.0",
    "styd-less-base": "^1.0.5",
    "vue": "^2.6.6",
    "vue-modal-router": "^0.2.9",
    "vue-router": "^3.0.1",
    "vue-router-multiguard": "^1.0.3",
    "vue-rx": "^6.1.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^2.2.1",
    "@types/nprogress": "^0.0.29",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.4.1",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.2.2",
    "less": "^2.7.2",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "rxact": "^1.0.0-beta.0",
    "style-resources-loader": "^1.2.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-template-compiler": "^2.6.6",
    "webpack-external-vendor-plugin": "^1.0.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ],
    "*.ts": [
      "npm run lint",
      "git add"
    ],
    "*.vue": [
      "npm run lint",
      "git add"
    ]
  }
