{
  "name": "vue-antd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "inspect": "vue-cli-service inspect > inspect.js",
    "lint": "vue-cli-service lint",
    "dev": "npm run serve",
    "serve": "vue-cli-service serve & gulp dev",
    "build": "vue-cli-service build",
    "ci": "yarn install && yarn build",
    "push:rx": "git subtree push git@gitlab.styd.cn:fe/packages/rx-state.git --prefix=rx-state master",
    "push:vsa": "git subtree push git@gitlab.styd.cn:fe/packages/vue-service-app.git --prefix=vue-service-app master"
  },
  "dependencies": {
    "@abraham/reflection": "^0.5.1",
    "@types/js-cookie": "^2.2.1",
    "@types/lodash-es": "^4.17.3",
    "@types/nprogress": "^0.0.29",
    "ant-design-vue": "^1.3.5",
    "fullcalendar-scheduler": "^1.9.4",
    "js-cookie": "^2.2.0",
    "nprogress": "^0.2.0",
    "qs": "^6.6.0",
    "rxjs": "^6.4.0",
    "styd-less-base": "^1.0.5",
    "vue": "^2.6.6",
    "vue-full-calendar": "^2.7.0",
    "vue-fullcalendar": "^1.0.9",
    "vue-modal-router": "^0.2.9",
    "vue-router": "^3.0.2",
    "vue-router-multiguard": "^1.0.3",
    "vue-rx": "^6.1.0",
    "vuedraggable": "^2.19.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/js-cookie": "^2.2.1",
    "@types/lodash-es": "^4.17.3",
    "@types/nprogress": "^0.0.29",
    "@types/qs": "^6.5.2",
    "@vue/cli-plugin-babel": "^3.1.0",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-typescript": "^3.1.0",
    "@vue/cli-service": "^3.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "const": "^1.0.0",
    "eslint": "^5.15.1",
    "eslint-plugin-vue": "^5.2.2",
    "git-rev-sync": "^1.12.0",
    "globby": "^9.1.0",
    "gulp": "^4.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "less-plugin-glob": "^3.0.0",
    "lint-staged": "^8.1.5",
    "lodash-es": "^4.17.11",
    "rxjs": "^6.4.0",
    "typescript": "^3.2.1",
    "vue-template-compiler": "^2.5.21",
    "webpack-external-vendor-plugin": "^1.0.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ],
    "*.ts": [
      "npm run lint",
      "git add"
    ],
    "*.vue": [
      "npm run lint",
      "git add"
    ]
  }
}
